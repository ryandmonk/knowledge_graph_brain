{
  "name": "knowledge-graph-brain-e2e-tests",
  "version": "0.19.0",
  "description": "Comprehensive Playwright E2E test suite for Knowledge Graph Brain",
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "test:ui": "playwright test --ui",
    "test:api": "playwright test --project=api-tests",
    "test:visual": "playwright test --project=visual",
    "test:performance": "playwright test --project=performance",
    "test:mobile": "playwright test --project=mobile-chrome --project=mobile-safari",
    "test:cross-browser": "playwright test --project=chromium --project=firefox --project=webkit",
    "show-report": "playwright show-report",
    "install-browsers": "playwright install"
  },
  "devDependencies": {
    "@playwright/test": "^1.40.0",
    "@types/node": "^20.0.0",
    "dotenv": "^16.0.3",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "neo4j-driver": "^5.0.0",
    "axios": "^1.6.0"
  }
}